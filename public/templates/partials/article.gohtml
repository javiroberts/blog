{{define "article"}}
<div class="mt-6 flex flex-col items-center justify-center">
    <h1 class="text-2xl text-center text-slate-700 font-medium mx-12 mt-4 mb-12 underline underline-offset-4 decoration-double lg:text-3xl">{{.Title}}</h1>
    <article class="prose prose prose-zinc w-4/5">
        {{.Article}}
    </article>
</div>
{{end}}